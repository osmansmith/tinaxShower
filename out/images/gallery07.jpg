<svg width="800" height="800" viewBox="0 0 800 800" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="800" fill="#F8FAFC"/>
  
  <!-- Ceramic Wall -->
  <rect x="100" y="100" width="600" height="600" fill="#FFFFFF" stroke="#E2E8F0" stroke-width="2"/>
  
  <!-- Ceramic Tiles Grid -->
  <defs>
    <pattern id="ceramicTiles" width="60" height="60" patternUnits="userSpaceOnUse">
      <rect width="60" height="60" fill="#F1F5F9" stroke="#CBD5E1" stroke-width="2"/>
      <rect x="5" y="5" width="50" height="50" fill="#FFFFFF" stroke="#E2E8F0" stroke-width="1"/>
    </pattern>
  </defs>
  
  <rect x="100" y="100" width="600" height="600" fill="url(#ceramicTiles)"/>
  
  <!-- Premium Accent Tiles -->
  <g fill="#3B82F6" opacity="0.8">
    <rect x="220" y="220" width="60" height="60"/>
    <rect x="340" y="220" width="60" height="60"/>
    <rect x="460" y="220" width="60" height="60"/>
    <rect x="580" y="220" width="60" height="60"/>
  </g>
  
  <!-- Grout Lines (Enhanced) -->
  <g stroke="#94A3B8" stroke-width="3" opacity="0.6">
    <line x1="100" y1="160" x2="700" y2="160"/>
    <line x1="100" y1="220" x2="700" y2="220"/>
    <line x1="100" y1="280" x2="700" y2="280"/>
    <line x1="100" y1="340" x2="700" y2="340"/>
    <line x1="100" y1="400" x2="700" y2="400"/>
    <line x1="100" y1="460" x2="700" y2="460"/>
    <line x1="100" y1="520" x2="700" y2="520"/>
    <line x1="100" y1="580" x2="700" y2="580"/>
    <line x1="100" y1="640" x2="700" y2="640"/>
  </g>
  
  <!-- Quality Badge -->
  <circle cx="650" cy="150" r="40" fill="#059669"/>
  <text x="650" y="160" font-family="Arial" font-size="16" font-weight="bold" text-anchor="middle" fill="#FFFFFF">PREMIUM</text>
  
  <!-- Shine Effect -->
  <g fill="#FFFFFF" opacity="0.3">
    <ellipse cx="300" cy="300" rx="50" ry="20" transform="rotate(45 300 300)"/>
    <ellipse cx="500" cy="400" rx="40" ry="15" transform="rotate(45 500 400)"/>
  </g>
  
  <text x="400" y="750" font-family="Arial" font-size="24" font-weight="bold" text-anchor="middle" fill="#1F2937">Revestimiento Cer√°mico</text>
</svg>